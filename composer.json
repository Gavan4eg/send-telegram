{
    "name": "gavan4eg1/sendtelegram",
    "description": "Send Message Telegram",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexey C",
            "email": "siteruslana@gmail.com",
            "homepage": "https://example1.com"
        }
    ],
    "homepage": "https://github.com/Gavan4eg/send-telegram",
    "keywords": ["Laravel", "SendTelegram"],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "~7|~8",
        "gavan4eg/sendtelegram": "1.0.*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Gavan4eg\\SendTelegram\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gavan4eg\\SendTelegram\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gavan4eg\\SendTelegram\\SendTelegramServiceProvider"
            ],
            "aliases": {
                "SendTelegram": "Gavan4eg\\SendTelegram\\Facades\\SendTelegram"
            }
        }
    }
}
